<!DOCTYPE html>
<html>
<head>
    <title>Dutch Treat: @ViewBag.Title</title>
    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" rel="stylesheet" />
</head>
<body>
    <header class="container">
        <nav class="navbar navbar-dark bg-dark navbar-expand-md">
        <h1 class="navbar-brand">Welcome to Dutch Treat</h1>
            <ul class="navbar-nav ">
                <li class="navitem"><a class="nav-link" asp-controller="App" asp-action="Index">Home</a></li>
                <li class="navitem"><a class="nav-link" asp-controller="App" asp-action="Contact">Contact</a></li>
                <li class="navitem"><a class="nav-link" asp-controller="App" asp-action="About">About</a></li>
            </ul>
        </nav>
    </header>
    <section class="container">
        <h2>@ViewBag.Title</h2>
        @RenderBody()
    </section>
    <footer class="container">
        <div class="text-center">Copyright 2021 Dutch Treat LLC</div>
    </footer>
    <script src="~/lib/jquery/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/js/index.js"></script>
    @RenderSection("Scripts",false)
</body>
</html>